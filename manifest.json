{
    "name": "IMDB Netflix Implementation",
    "version": "1.0",
    "description": "Implemneting IMDB ratings on netflix.",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches":[
                "https://www.netflix.com/*"
            ],
            "js":[
                "script.js"
            ]
        }
    ],
    "background":{
        "scripts":["background.js"]
    }
    
}